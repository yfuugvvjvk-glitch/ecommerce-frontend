{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://ecommerce-fullstack-3y1b.onrender.com"
  },
  "functions": {
    "app/api/**/*.js": {
      "maxDuration": 30
    }
  }
}